{
  "name": "galvanize-admission-portal",
  "version": "1.0.0",
  "description": "Admissions portal application for Galvanize students to be able to apply.",
  "main": "index.js",
  "repository": {
  "type": "git",
  "url": "https://github.com/Galvanize-IT/admissions-portal.git"
},
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel -d ./dist ./server -s",
    "start": "npm run clean && npm run build-babel && node ./dist/index.js",
    "heroku-postbuild": "cd client && npm install && npm run build:production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "joi": "^14.0.3",
    "jsforce": "^1.9.1",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.15.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.6.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.5"
  }
}
